{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product_name": {
      "type": "string"
    },
    "colors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::color.color",
      "mappedBy": "products"
    },
    "product_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "name": {
      "type": "uid",
      "targetField": "product_name"
    },
    "sizes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::size.size",
      "inversedBy": "products"
    },
    "surface_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::surface-type.surface-type",
      "inversedBy": "products"
    }
  }
}
